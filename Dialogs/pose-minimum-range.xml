            <row>0</row>
            <col>1</col>
            <halign>fill</halign>
            <stretch>true</stretch>
            <!-- <width>150</width> -->
            <!-- <height>25</height> -->
            <property>/fdm/jsbsim/Creare/pose-name</property>
            <value>Joystick</value>
            <value>Neutral</value>
            <value>Box</value>
            <value>Left Translation</value>
            <value>Right Translation</value>
            <value>Anterior Translation</value>
            <value>Posterior Translation</value>
            <value>Left Dorsoventral</value>
            <value>Right Dorsoventral</value>
            <value>Dorsal</value>
            <value>Ventral</value>
            <value>Parachute</value>
            <binding>
